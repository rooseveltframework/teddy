{
  "name": "teddy",
  "description": "Teddy Templating Engine",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.4.10",
  "homepage": "https://github.com/kethinov/teddy",
  "license": "CC-BY-4.0",
  "main": "teddy.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true,
  "dependencies": {
  },
  "devDependencies": {
    "eslint": "3.13.1",
    "mocha": "3.2.0",
    "chai": "3.5.0",
    "chai-string": "1.3.0",
    "istanbul": "0.4.5",
    "mocha-lcov-reporter": "1.3.0",
    "coveralls": "2.12.0",
    "cross-env": "4.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "curly": 2,
      "semi": 2,
      "strict": [2, "never"],
      "quotes": [2, "single"],
      "no-trailing-spaces": ["error", { "skipBlankLines": false }],
      "camelcase": [2, {"properties": "always"}],
      "comma-spacing": [2, {"before": false, "after": true}],
      "indent": [2, 2, {"VariableDeclarator": 2, "SwitchCase": 1}],
      "eol-last": 0,
      "handle-callback-err": 0,
      "no-use-before-define": [2, "nofunc"],
      "no-unused-vars": [2, {"vars": "all", "args": "none"}],
      "no-console": 0,
      "no-mixed-requires": 0,
      "one-var": [2, "always"],
      "dot-notation": 0,
      "no-path-concat": 0,
      "no-alert": 0,
      "brace-style": [2, "stroustrup"],
      "keyword-spacing": ["error", { "before": true, "after": true }],
      "space-before-blocks": ["error"]
    }
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive test",
    "eslint": "eslint .",
    "cover": "cross-env NODE_ENV=cover istanbul cover _mocha --recursive test"
  },
  "pre-commit": [
    "test",
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kethinov/teddy.git"
  },
  "keywords": [
    "teddy.js",
    "node.js",
    "teddy",
    "template",
    "view"
  ],
  "_from": "teddy@*",
  "gitHead": "1dbbc4c73d7c8878cbe7fc0ba527c91018b4cea7"
}
